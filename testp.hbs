{{#each foods}}

  {{#each this}}
  <div class="row">
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img  class="img-responsive" src="{{this.link}}" alt="..." >
      <div class="caption">
        <h3>{{this.title}}</h3>
        <p class="description">{{this.description}}</p>
        <div class="clearfix">
          <div class="price pull-left">
           &#8377 {{this.price}}
          </div>
          <a href="#" class="btn btn-success pull-right" role="button">Order </a>
          </div>
      </div>
    </div>
  </div>
  {{/each}}
</div>
  
  {{/each}}
  
