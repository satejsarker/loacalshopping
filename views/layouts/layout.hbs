<script type="text/javascript">
$(document).ready(function(){
    $('input.typeahead').typeahead({
        name: 'countries',
        remote: 'http://192.168.0.104:3000/search?key=%QUERY',
       // remote: 'http://localhost:3000/search?key=%QUERY',
        limit: 6
    });
$("input.city").typeahead({
        name: 'citys',
         remote: 'http://192.168.0.104:3000/location?key=%QUERY',
        //remote: 'http://localhost:3000/location?key=%QUERY',
        limit: 6
    });
});
</script>
<style type="text/css">
		.bs-example{
			font-family: sans-serif;
			position: relative;
			margin: 50px;
		}
		.typeahead, .tt-query, .tt-hint {
			border: 2px solid #CCCCCC;
			border-radius: 8px;
			font-size: 24px;
			height: 30px;
			line-height: 30px;
